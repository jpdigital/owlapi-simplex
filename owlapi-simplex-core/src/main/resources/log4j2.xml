<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="de.jpdigital.owl.apigenerator.core.OntologyOwlApi"
                level="DEBUG">
        </Logger>
        <Logger name="de.jpdigital.owl.apigenerator.core.IriBundleBuilder"
                level="DEBUG">
        </Logger>
        <Logger name="de.jpdigital.owl.apigenerator.core.IriConstantsGenerator"
                level="DEBUG">
        </Logger>
        <Logger name="de.jpdigital.owl.apigenerator.core.RepositoryGenerator"
                level="DEBUG">
        </Logger>
        <Root level="WARN">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
